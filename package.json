  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/*.spec.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "scripts": {
    "build:client": "webpack --config ./webpack/webpack.client.js",
    "build:server": "webpack --config ./webpack/webpack.server.js",
    "bundle": "npm run clean && npm run build:server && npm run build:client",
    "bundle:min": "npm run clean && npm run build:server && set NODE_ENV=production build:client",
    "clean": "rimraf build/*",
    "lint": "tslint src/**/*.ts{,x}",
    "run": "node build/server",
    "run:dev": "webpack-dev-server --config ./webpack/webpack.dev.js --inline --hot",
    "start": "npm run clean && npm run build:server && run-p run run:dev",
    "test": "jest"
  },
